{{!-- List Leaderboard Start --}}
<h1 class="page-header">Leaderboard</h1>
<div class="leaderB">
    <div class="panel-heading">
        <h3 class="panel-title">Task List <span class="badge">5</span></h3>
    </div>
    <div class="panel-body">
        <ul class="list-group">
            {{#each taskList}}
            <li class="list-group-item">
                {{title}}
            </li>
            {{/each}}
        </ul>
    </div>
    <div class="panel-footer">
        Last Updated : {{lastAdded}}
    </div>

    {{{link "Home" href=index.handlebars class="btn"}}}
    {{{link "Play" href=game.handlebars class="btn"}}}
</div>
{{!-- Task List End --}}
